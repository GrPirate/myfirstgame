{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"es2016",
			"es2018.promise",
			"dom"
		],
		"module": "commonjs",
		"watch": false,
		"sourceMap": false,
		"rootDir":"./src",
		"outDir": "./out",
		"allowSyntheticDefaultImports": true
	},
	"include": [
			"src/**/*"
	],
	"exclude": [
		"out"
	]
}